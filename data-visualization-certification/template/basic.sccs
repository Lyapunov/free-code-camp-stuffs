html, body,
   max-width : 100%
   height    : 100%
   overflow-x: hidden

.noselect
   -webkit-touch-callout: none
   -webkit-user-select: none
   -khtml-user-select: none
   -moz-user-select: none
   -ms-user-select: none
   user-select: none
                             
.big-font
   font-size: 8vh

.medium-font
   font-size: 5vh
   text-decoration: underline

